{
  "compilerOptions": {
    "module": "ESNext",
    "target": "es6",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "noErrorTruncation": true,
    "noEmit": true,
    "declaration": true,
    "noUncheckedIndexedAccess": true
  }
}